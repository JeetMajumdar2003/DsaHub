    "generate:data": "node ./scripts/generate-codebase-data.mjs"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "vite": "^5.2.0",
    "vitest": "^1.6.0"
  }
}
