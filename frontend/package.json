{
  "name": "cpp-dsa-showcase",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Beautiful interactive showcase for the C++ DSA codebase",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "format": "prettier --write \"**/*.{js,css,html,json,md}\"",
    "generate:data": "node ./scripts/generate-codebase-data.mjs"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.3",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "vite": "^5.2.0",
    "vitest": "^1.6.0"
  }
}
